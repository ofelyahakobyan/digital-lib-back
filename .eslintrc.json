{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-shadow": "off",
    "import/prefer-default-export": "off",
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ],
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ],
    "object-curly-newline": [
      "error",
      {
        "multiline": true
      }
    ]
  }
}
